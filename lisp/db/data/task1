(PROG1 (SETF TASK225 (MAKE-INSTANCE 'TASK)) (SETF (SLOT-VALUE TASK225 'DATE-DUE) (LIST->DATE '(5 5 1955))) (SETF (SLOT-VALUE TASK225 'STATUS) "Active") (SETF (SLOT-VALUE TASK225 'DATE-OPEN) (LIST->DATE '(10 12 1993))) (SETF (SLOT-VALUE TASK225 'SYMBOL) 'TASK225) (SETF (SLOT-VALUE TASK225 'SUBJECT) "test 1"))(INSERT-ITEM TASK TASK225)