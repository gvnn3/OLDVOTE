;; Macintosh HD:lisp:db:data:group

;; list of command aliases.
(SETF (db-aliases GROUP)  (list  (list  'AS 'APPEND 'STANCES)
 (list  'HC 'HEADERS 'CURRENT)
 (list  'N 'STANCES)
)
)


;; Items for Database: GROUP
(defvar GROUP.2997)
(prog1 (setf GROUP.2997 (make-instance 'GROUP))
(setf (slot-value GROUP.2997 'NAME)  "AFL-CIO") 
(setf (slot-value GROUP.2997 'ENGLISH)  "the AFL-CIO") 
(setf (slot-value GROUP.2997 'CON-ENGLISH)  "the AFL-CIO labor bosses") 
(setf (slot-value GROUP.2997 'SYNONYMS)  (list  'AFLCIO)
) 
(setf (slot-value GROUP.2997 'ISA)  (list  'UNION)
) 
(setf (slot-value GROUP.2997 'STATUS)  "Active") 
(setf (slot-value GROUP.2997 'DATE-OPEN) (list->date '(8 26 1988))) 
(setf (slot-value GROUP.2997 'SYMBOL)  'GROUP.2997) 
(setf (slot-value GROUP.2997 'KEYWORDS)  (list  'COPE)
) )
(insert-item GROUP GROUP.2997)
