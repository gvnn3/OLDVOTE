(defun undo-field (slot args)    (ignore args)  (let* ((selector (stype-selector (db-struct *current-db*) slot))         (items (db-current-items *current-db*))         (old-values (MAPCAR #'selector items)))    #'(LAMBDA ()       (MAPC #'       #'(LAMBDA (item value)         (pop-update *current-db*)         (format t "~%Resetting ~A slot of ~A to ~A~%" slot (id item) value)         (SETF (selector item) value))       items old-values))))